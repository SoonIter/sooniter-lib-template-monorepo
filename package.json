{
  "name": "sooniter-lib-template",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@7.5.0",
  "description": "自用的库模版",
  "author": {
    "name": "SoonIter",
    "email": "1730214056@qq.com"
  },
  "license": "MIT",
  "repository": "https://github.com/SoonIter/sooniter-lib-template",
  "bugs": {
    "url": "https://github.com/SoonIter/sooniter-lib-template/issues"
  },
  "keywords": [
    "react",
    "usePagination",
    "vue"
  ],
  "main": "dist/react/usePagination.js",
  "types": "dist/react/usePagination.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts",
    "release": "nr prerelease && nr build && npm publish",
    "prerelease": "nr lint:fix && nr test:once",
    "test": "vitest",
    "test:once": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.*",
    "lint:fix": "eslint src/**/*.* --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.3",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/react": "^18.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "@umijs/fabric": "^2.12.2",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.15.1",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "happy-dom": "^6.0.2",
    "jsdom": "^19.0.0",
    "lint-staged": "13.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.2.0",
    "simple-git-hooks": "^2.8.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.18.0",
    "vue": "^3.2.33"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
