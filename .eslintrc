{
  "extends": "@antfu",
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "no-console": "off",
    "no-restricted-imports": [
      "error",
      {
        "paths": [
          "path"
        ]
      }
    ],
    "eslint-comments/no-unlimited-disable": "off",
    "import/no-named-as-default": "off"
  },
  "overrides": [
    {
      "files": "packages/**/*.*",
      "rules": {
        "no-restricted-imports": [
          "error",
          {
            "paths": [
              "vitest",
              "path"
            ]
          }
        ]
      }
    },
    {
      "files": "packages/vitest/**/*.*",
      "rules": {
        "no-restricted-globals": [
          "error",
          "setTimeout",
          "clearTimeout",
          "setInterval",
          "clearInterval",
          "performance"
        ]
      }
    }
  ]
}